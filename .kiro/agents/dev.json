{
  "name": "dev",
  "description": "Agent for bonvoy - Plugin-based release automation tool for npm packages and monorepos",
  "prompt": "file://../prompts/dev.md",
  "resources": ["file://README.md", "file://.kiro/specs/v1/requirements.md"],
  "tools": [
    "read",
    "write",
    "shell",
    "grep",
    "glob",
    "web_search",
    "web_fetch",
    "introspect",
    "thinking",
    "code"
  ],
  "toolAliases": {},
  "allowedTools": [
    "read",
    "shell",
    "grep",
    "glob",
    "web_search",
    "web_fetch",
    "introspect",
    "thinking",
    "code"
  ],
  "toolsSettings": {
    "write": {
      "allowedPaths": [".kiro/**", "packages/**", "README.md", "package.json"]
    },
    "shell": {
      "autoAllowReadonly": true
    }
  }
}
